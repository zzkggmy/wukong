<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:background="@color/gray_fp"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <include layout="@layout/nav_bar_layout" />
    <ListView
        android:id="@id/morefp_listview"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp1"
        android:divider="@color/gray_fp"
        android:dividerHeight="@dimen/dp1" />
    <GridView
        android:id="@id/mf_gridview"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:horizontalSpacing="@dimen/dp1"
        android:verticalSpacing="@dimen/dp3"
        android:columnWidth="@dimen/dp1"
        android:numColumns="3" />
</LinearLayout>